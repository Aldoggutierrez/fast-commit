{
	"name": "commitrapido",
	"displayName": "Fast commiter",
	"description": "esta extension de vs code hace add a todos los archivos del repositorio ademas de un commit, push y pul del repositorio remoto ",
	"version": "0.0.1",
	"publisher": "Aldogtz",
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"icon": "images/icon.png",
	"activationEvents": [
		"onCommand:identificador.commit"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "identificador.commit",
				"title": "Hacer Add,Commit,Push y Pull"
			}
		],
		"keybindings": [
			{
			  "command": "identificador.commit",
			  "key": "Shift+c",
			  "mac": "Shift+c",
			  "when": "editorTextFocus"
			}
		  ]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.60.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3",
		"@vscode/test-electron": "^1.6.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Aldoggutierrez/fast-commit.git"
	  }
}
